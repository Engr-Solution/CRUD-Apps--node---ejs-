<% for(let i = 0; i < users.length; i++){%>
<tr>
  <th scope="row"><%= i + 1 %></th>
  <td><%= users[i].name %></td>
  <td><%= users[i].email %></td>
  <td><%= users[i].gender %></td>
  <td><%= users[i].status %></td>
  <td>
    <span>
      <a href="/update_user?id=<%= users[i]._id %>" class="text-decoration-none">
        <i class="bi bi-pencil-fill me-3 fw-bold fs-5 update"></i>
      </a>
      <a class="delete text-decoration-none" data-id='<%= users[i]._id %>'>
        <i class="bi bi-trash-fill fw-bold fs-5 deleteIcon"></i>
      </a>
    </span>
  </td>
</tr>
<%}%>
